FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.375;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.375;
    }

    outlet
    {
        type            zeroGradient;
    }

    wall_membrane
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }

    wall_up
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }
	sides
	{
	   type             empty;
	}

}


